<%- include("./partials/header.ejs") %>
<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label class="" for="">Title</label>
    <input class="form-control" type="text" name="postTitle" />
    <label class="" for="">Post</label>
    <textarea
      method="post"
      class="form-control"
      type="texarea"
      rows="8"
      cols="80"
      name="postBody"
    ></textarea>
  </div>
  <button class="btn btn-primary mt-3" type="submit" name="button">
    Publish
  </button>
</form>
<%-include("./partials/footer.ejs") %>
